{
  "name": "My First Extension",
  "version": "1.1.10",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": ["./js/jquery-1.8.1.min.js", "./js/foreground.js"],
            "css": ["./css/alc.css"],
            "run_at": "document_end"
        }
  ],
  "background": {
    "scripts": ["./js/jquery-1.8.1.min.js", "./js/underscore-min.js", "./js/backbone-min.js","./js/fetchimi.js"]
  },
  "permissions": [
    "http://eow.alc.co.jp/"
  ]
}
